<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN"
	"http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>

	<typeHandlers>
		<typeHandler handler="org.hbhk.aili.orm.server.handler.BooleanTypeHandler"
			javaType="Boolean" jdbcType="CHAR" />
	</typeHandlers>
	<plugins>
		<plugin interceptor="org.hbhk.aili.orm.server.interceptor.OffsetLimitInterceptor">
			<property name="dialectClass"
				value="org.hbhk.aili.orm.server.mybatis.dialect.MySQLDialect" />
		</plugin>
	</plugins>
</configuration>